{
  "private": true,
  "version": "0.0.1",
  "description": "Prototype vocal recording / monitoring app",
  "main": "",
  "scripts": {
    "postinstall": "rm -rf dist/* && mkdir dist/assets && mkdir dist/assets/instrumentals && mkdir dist/assets/webworkers",
    "start": "node server.js --env=dev",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "posttest": "npm run lint",
    "serve": "node server.js --env=dev",
    "serve:dist": "node server.js --env=dist",
    "dist": "npm run copy && webpack --env=dist && npm run copy:audio && npm run copy:webworkers",
    "lint": "eslint ./src",
    "copy": "copyfiles -f ./src/index.html ./src/favicon.ico ./dist",
    "copy:audio": "copyfiles -f ./src/instrumentals/* ./dist/assets/instrumentals/",
    "copy:webworkers": "copyfiles -f ./src/lib/user-media-recorder/webworkers/*  ./dist/assets/webworkers/",
    "clean": "rimraf dist/*",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags"
  },
  "repository": "",
  "keywords": [],
  "author": "Your name here",
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.5.0",
    "bower-webpack-plugin": "^0.1.9",
    "copy-webpack-plugin": "^4.0.1",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.23.0",
    "eslint": "^3.0.0",
    "eslint-loader": "^1.0.0",
    "eslint-plugin-react": "^6.0.0",
    "file-loader": "^0.9.0",
    "glob": "^7.0.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^3.0.0",
    "node-sass": "^4.5.0",
    "null-loader": "^0.1.1",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.0.0",
    "react-hot-loader": "^1.2.9",
    "rimraf": "^2.4.3",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "aws-sdk": "^2.28.0",
    "body-parser": "^1.17.1",
    "core-js": "^2.0.0",
    "dotenv": "^4.0.0",
    "material-ui": "^0.17.1",
    "normalize.css": "^4.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-motion-ui-pack": "^0.10.2",
    "react-tap-event-plugin": "^2.0.1"
  }
}
